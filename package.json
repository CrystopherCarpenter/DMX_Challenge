{
  "name": "backend-code-challenge",
  "version": "1.0.0",
  "repository": "git@gitlab.com:code-tests-dmx/backend-code-challenge.git",
  "author": "",
  "license": "MIT",
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev": "nodemon src/index.ts",
    "test": "jest --config package.json",
    "lint": "eslint src --ext .ts"
  },
  "devDependencies": {
    "@codingsans/eslint-config": "4.0.0",
    "@types/bcrypt": "5.0.0",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/jest": "26.0.23",
    "@types/jsonwebtoken": "8.5.8",
    "@types/node": "17.0.34",
    "eslint": "6.8.0",
    "jest": "26.6.3",
    "nodemon": "^2.0.16",
    "prettier": "1.19.1",
    "prisma": "4.8.0",
    "ts-jest": "26.5.5",
    "ts-node": "10.7.0",
    "typescript": "4.2.4"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/*.factory.ts",
      "!src/commands/*"
    ],
    "coverageReporters": [
      "html",
      "json"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "@prisma/client": "4.8.0",
    "axios": "1.2.2",
    "bcrypt": "5.1.0",
    "config": "3.3.8",
    "cors": "2.8.5",
    "dotenv": "16.0.1",
    "dotenv-expand": "8.0.3",
    "express": "4.18.1",
    "http-status": "1.5.2",
    "joi": "17.6.0",
    "jsonwebtoken": "8.5.1",
    "morgan": "1.10.0"
  }
}
